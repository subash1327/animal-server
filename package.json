{
  "name": "animal-server",
  "version": "1.0.0",
  "description": "A production-ready Express.js + TypeScript backend with PostgreSQL and PostGraphile.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:up": "docker-compose up -d",
    "migrate": "node-pg-migrate up",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "db:seed": "node seeds/seed.js",
    "strapi": "strapi develop"
  },
  "keywords": [
    "express",
    "typescript",
    "postgresql",
    "postgraphile",
    "strapi"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.15.2",
    "@types/node": "^20.4.8",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "knex": "^3.1.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.1",
    "pino": "^8.14.2",
    "pino-pretty": "^10.2.0",
    "postgraphile": "^4.13.0"
  }
}
